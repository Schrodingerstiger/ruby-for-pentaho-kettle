    <sql>--&#xa;-- Note&#x3a;&#xa;-- This step executes first, before the rest of the transformation starts. &#xa;-- It populates the H2 in-memory DB with some critters.&#xa;&#xa;DROP TABLE IF EXISTS critters&#x3b;&#xa;&#xa;CREATE TABLE critters&#x28;&#xa;	ID INT PRIMARY KEY,&#xa;	CRITTER VARCHAR&#x28;100&#x29;&#xa;&#x29;&#x3b;&#xa;&#xa;INSERT INTO critters VALUES &#x28;1,&#x27;cat&#x27;&#x29;&#x3b;&#xa;INSERT INTO critters VALUES &#x28;2,&#x27;dog&#x27;&#x29;&#x3b;&#xa;INSERT INTO critters VALUES &#x28;3, &#x27;ant&#x27;&#x29;&#x3b;&#xa;INSERT INTO critters VALUES &#x28;4, &#x27;badger&#x27;&#x29;&#x3b;&#xa;INSERT INTO critters VALUES &#x28;5, &#x27;bee&#x27;&#x29;&#x3b;&#xa;INSERT INTO critters VALUES &#x28;6, &#x27;camel&#x27;&#x29;&#x3b;&#xa;INSERT INTO critters VALUES &#x28;7, &#x27;chicken&#x27;&#x29;&#x3b;&#xa;INSERT INTO critters VALUES &#x28;8, &#x27;crow&#x27;&#x29;&#x3b;&#xa;INSERT INTO critters VALUES &#x28;9, &#x27;deer&#x27;&#x29;&#x3b;&#xa;INSERT INTO critters VALUES &#x28;10, &#x27;eagle&#x27;&#x29;&#x3b;&#xa;INSERT INTO critters VALUES &#x28;11, &#x27;falcon&#x27;&#x29;&#x3b;&#xa;INSERT INTO critters VALUES &#x28;12, &#x27;panda&#x27;&#x29;&#x3b;</sql>
